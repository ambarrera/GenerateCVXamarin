<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DynamicForm.CVPage">

	<ScrollView>
		<StackLayout Padding="10">
			<Label x:Name="labelNombre" FontSize="24" FontAttributes="Bold"/>
			<Label x:Name="labelApellidos" FontSize="24" FontAttributes="Bold"/>
			<Label x:Name="labelFechaNacimiento" />
			<Label x:Name="labelNacionalidad" />
			<Label x:Name="labelAptitudes" />
			<Label x:Name="labelHabilidades" />
			<Label x:Name="labelLenguajes" />
			<Label x:Name="labelPerfil" />
			<Label x:Name="labelExperienciaLaboral" />
			<Label x:Name="labelEducacion" />
		</StackLayout>
	</ScrollView>
</ContentPage>